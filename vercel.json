{
  "version": 2,
    "builds": [
    {
      "src": "coopcraft/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" , "runtime": "python-3.12" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "coopcraft/wsgi.py",
      "methods": ["GET","POST","PUT","PATCH","DELETE","OPTIONS"]
    }
  ]
}
